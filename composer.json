{
	"name": "david63/biblequotes",
	"type": "phpbb-extension",
	"description": "Adds a bible quotes feature to phpBB using Bible Gateway.",
	"version": "2.1.0-RC1",
	"time": "2019-09-08",
	"keywords": [
		"phpbb",
		"extension",
		"bible",
		"quote"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Extension Developer"
		}
    ],
	"require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Bible quotes",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/biblequotes_version_file.json"
    	}
	}
}
